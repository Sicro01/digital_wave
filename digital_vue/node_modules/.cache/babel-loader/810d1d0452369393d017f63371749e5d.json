{"ast":null,"code":"import PlanCardShow from '@/components/PlanCardShow';\nimport PlanEditDialog from '@/components/dialogs/PlanEditDialog';\nimport axios from 'axios';\nexport default {\n  name: 'PlanCardView',\n  components: {\n    PlanCardShow,\n    PlanEditDialog\n  },\n  data() {\n    return {\n      plans: [],\n      toggleNewPlan: false,\n      open_dialog: {},\n      editId: -1,\n      editPlan: {\n        id: 0,\n        name: '',\n        description: ''\n      }\n    };\n  },\n  mounted() {\n    this.getPlans();\n    document.title = 'Home | Digital Wave';\n  },\n  methods: {\n    async getPlans() {\n      await axios.get('api/v1/plans/').then(response => {\n        this.plans = response.data;\n        console.log('plan data:', this.plans);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    save(editPlan) {\n      console.log('save updated plan: ', editPlan);\n    },\n    openEditDialog(index, editPlan) {\n      console.log('planId: ', editPlan);\n      console.log('Before PlanCardView OpenDialog', this.open_dialog[index], index);\n      this.$set(this.open_dialog, index, true);\n      console.log('After PlanCardView OpenDialog', this.open_dialog[index], index);\n      this.editPlan = editPlan;\n    },\n    closeDialog(index) {\n      this.$set(this.open_dialog, index, false);\n    }\n  }\n};","map":{"version":3,"mappings":"AAsCA;AACA;AACA;AAEA;EACAA;EACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;QACAC;QACAV;QACAW;MACA;IAEA;EACA;EACAC;IACA;IACAC;EACA;EACAC;IACA;MACA,YACAC,qBACAC;QACA;QACAC;MACA,GACAC;QACAD;MACA;IACA;IACAE;MACAF;IACA;IACAG;MACAH;MACAA;MACA;MACAA;MACA;IACA;IACAI;MACA;IACA;EACA;AACA","names":["name","components","PlanCardShow","PlanEditDialog","data","plans","toggleNewPlan","open_dialog","editId","editPlan","id","description","mounted","document","methods","get","then","console","catch","save","openEditDialog","closeDialog"],"sourceRoot":"src/views","sources":["PlanCardView.vue"],"sourcesContent":["<template>\n    <v-container fluid>\n        <v-row\n            class=\"primary--text font-weight-black text-h3\">\n            <v-col md=\"6\" offset=\"3\" class=\"text-center\">\n                <span>Plans</span>\n            </v-col>\n            <v-spacer></v-spacer>\n            <v-col cols=\"2\" class=\"text-right\">\n                <v-btn\n                    class=\"mr-2 mb-3 primary rounded-0 text-capitalize text-body-1\"\n                    dark\n                    @click=\"toggleNewPlan = !toggleNewPlan\"\n                    depressed>\n                    New Plan\n                </v-btn>\n            </v-col>\n        </v-row>\n        <v-row class=\"mb-5 mx-4 justify-space-around\">\n            <v-col\n                v-for=\"(plan, index) in plans\" :key=\"plan.id\"\n                lg=\"3\"\n                md=\"4\"\n                sm=12>\n                <PlanCardShow\n                    :plan=\"plan\"\n                    @open-dialog=\"openEditDialog(index, plan)\" />\n                <PlanEditDialog\n                    :dialog=\"open_dialog[index]\"\n                    :editId=\"editId\"\n                    :plan=\"editPlan\"\n                    @close-dialog=\"closeDialog(index)\"\n                    @save=\"save(plan)\" />\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n<script>\nimport PlanCardShow from '@/components/PlanCardShow'\nimport PlanEditDialog from '@/components/dialogs/PlanEditDialog'\nimport axios from 'axios'\n\nexport default {\n    name: 'PlanCardView',\n    components: {\n        PlanCardShow,\n        PlanEditDialog,\n    },\n    data() {\n        return {\n            plans: [],\n            toggleNewPlan: false,\n            open_dialog: {},\n            editId: -1,\n            editPlan: {\n                id: 0,\n                name: '',\n                description: '',\n            },\n\n        }\n    },\n    mounted() {\n        this.getPlans()\n        document.title = 'Home | Digital Wave'\n    },\n    methods: {\n        async getPlans() {\n            await axios\n                .get('api/v1/plans/')\n                .then(response => {\n                    this.plans = response.data\n                    console.log('plan data:', this.plans)\n                })\n                .catch(error => {\n                    console.log(error)\n                })\n        },\n        save(editPlan) {\n            console.log('save updated plan: ', editPlan)\n        },\n        openEditDialog(index, editPlan) {\n            console.log('planId: ', editPlan)\n            console.log('Before PlanCardView OpenDialog', this.open_dialog[index], index)\n            this.$set(this.open_dialog, index, true)\n            console.log('After PlanCardView OpenDialog', this.open_dialog[index], index)\n            this.editPlan = editPlan\n        },\n        closeDialog(index) {\n            this.$set(this.open_dialog, index, false)\n        },\n    }\n}\n</script>\n<style lang=\"scss\">\n\n</style>"]},"metadata":{},"sourceType":"module"}