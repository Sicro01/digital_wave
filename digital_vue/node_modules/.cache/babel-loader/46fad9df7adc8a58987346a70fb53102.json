{"ast":null,"code":"import PlanCardShow from '@/components/PlanCardShow';\nimport PlanEditDialog from '@/components/dialogs/PlanEditDialog';\nimport axios from 'axios';\nexport default {\n  name: 'PlanCardView',\n  components: {\n    PlanCardShow,\n    PlanEditDialog\n  },\n  data() {\n    return {\n      plans: [],\n      toggleNewPlan: false,\n      open_dialog: false,\n      editId: -1,\n      newPlan: {\n        id: 0,\n        name: '',\n        description: ''\n      },\n      editPlan: {\n        id: 0,\n        name: '',\n        description: ''\n      }\n    };\n  },\n  mounted() {\n    this.getPlans();\n    document.title = 'Home | Digital Wave';\n  },\n  methods: {\n    async getPlans() {\n      await axios.get('api/v1/plans/').then(response => {\n        this.plans = response.data;\n        console.log('plan data:', this.plans);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    async save(editPlan) {\n      console.log('editPlan: ', editPlan);\n      await axios({\n        method: 'put',\n        url: `api/v1/plans/${editPlan.id}/`,\n        data: editPlan,\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      this.open_dialog = false;\n      const payload = {\n        text: `Successfully saved ${editPlan.name}`,\n        alerttype: \"success\",\n        contentclass: 'white--text'\n      };\n      this.$store.commit('showSnackBar', payload);\n    },\n    openEditDialog(editPlan) {\n      console.log('planId: ', editPlan);\n      console.log('Before PlanCardView OpenDialog', this.open_dialog);\n      this.editId = editPlan.id;\n      this.editPlan = editPlan;\n      this.open_dialog = true;\n      // this.$set(this.open_dialog, index, true)\n      console.log('After PlanCardView OpenDialog', this.open_dialog);\n    },\n    closeDialog() {\n      this.open_dialog = false;\n      this.editPlan - this.newPlan;\n      // this.$set(this.open_dialog, index, false)\n    }\n  }\n};","map":{"version":3,"mappings":"AAsCA;AACA;AACA;AAEA;EACAA;EACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;QACAC;QACAV;QACAW;MACA;MACAC;QACAF;QACAV;QACAW;MACA;IACA;EACA;EACAE;IACA;IACAC;EACA;EACAC;IACA;MACA,YACAC,qBACAC;QACA;QACAC;MACA,GACAC;QACAD;MACA;IACA;IACA;MACAA;MACA;QACAE;QACAC;QACAjB;QACAkB;UAAA;QAAA;MACA,GACAH;QACAD;MACA;MACA;MACA;QAAAK;QAAAC;QAAAC;MAAA;MACA;IACA;IACAC;MACAR;MACAA;MACA;MACA;MACA;MACA;MACAA;IAEA;IACAS;MACA;MACA;MACA;IACA;EACA;AACA","names":["name","components","PlanCardShow","PlanEditDialog","data","plans","toggleNewPlan","open_dialog","editId","newPlan","id","description","editPlan","mounted","document","methods","get","then","console","catch","method","url","headers","text","alerttype","contentclass","openEditDialog","closeDialog"],"sourceRoot":"src/views","sources":["PlanCardView.vue"],"sourcesContent":["<template>\n    <v-container fluid>\n        <v-row\n            class=\"primary--text font-weight-black text-h3\">\n            <v-col md=\"6\" offset=\"3\" class=\"text-center\">\n                <span>Plans</span>\n            </v-col>\n            <v-spacer></v-spacer>\n            <v-col cols=\"2\" class=\"text-right\">\n                <v-btn\n                    class=\"mr-2 mb-3 primary rounded-0 text-capitalize text-body-1\"\n                    dark\n                    @click=\"openEditDialog(newPlan)\"\n                    depressed>\n                    New Plan\n                </v-btn>\n            </v-col>\n        </v-row>\n        <v-row class=\"mb-5 mx-4 justify-space-around\">\n            <v-col\n                v-for=\"(plan, index) in plans\" :key=\"plan.id\"\n                lg=\"3\"\n                md=\"4\"\n                sm=12>\n                <PlanCardShow\n                    :plan=\"plan\"\n                    @open-dialog=\"openEditDialog(plan)\" />\n            </v-col>\n        </v-row>\n        <PlanEditDialog\n            :dialog=\"open_dia