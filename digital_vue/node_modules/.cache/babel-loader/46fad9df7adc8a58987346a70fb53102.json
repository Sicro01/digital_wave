 openEditDialog(editPlan) {\n            console.log('planId: ', editPlan)\n            console.log('Before PlanCardView OpenDialog', this.open_dialog)\n            this.editId = editPlan.id\n            this.editPlan = editPlan\n            this.open_dialog = true\n            // this.$set(this.open_dialog, index, true)\n            console.log('After PlanCardView OpenDialog', this.open_dialog)\n\n        },\n        closeDialog() {\n            this.open_dialog = false\n            this.editPlan - this.newPlan\n            // this.$set(this.open_dialog, index, false)\n        },\n    }\n}\n</script>\n<style lang=\"scss\">\n\n</style>"]},"metadata":{},"sourceType":"module"}