{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport CardIcons from '@/components/shared/CardIcons';\nexport default {\n  name: 'CountryCard',\n  components: {\n    CardIcons\n  },\n  data() {\n    return {\n      tableHeaders: [{\n        text: 'Country',\n        align: 'center',\n        sortable: false,\n        value: 'country',\n        class: \"text-subtitle-1 black--text\"\n      }, {\n        text: 'Include?',\n        align: 'start',\n        value: 'include',\n        class: \"text-subtitle-1 black--text\"\n      }, {\n        text: 'Budget',\n        align: 'center',\n        value: 'budget',\n        class: \"text-subtitle-1 black--text\"\n      }, {\n        text: 'Auto-allocate?',\n        align: 'start',\n        value: 'autoallocate',\n        class: \"text-subtitle-1 black--text\"\n      }],\n      tableItems: [],\n      tableItemLabels: [],\n      budgetValues: [],\n      locked: true,\n      valid: false,\n      rules: {\n        required: v => !!v || \"Required\",\n        notEmpty: v => v.toString().length > 0 || \"Budget should be between 0 and 10,000,000\",\n        inRange: v => Number.isInteger(Number(v)) && v >= 0 && v <= 10000000 || \"Budget should be between 0 and 10,000,000\"\n      }\n    };\n  },\n  computed: {\n    selectedPhase: {\n      get() {\n        return this.$store.state.selectedPhaseData.phase;\n      }\n    },\n    selectedStrategy: {\n      get() {\n        return this.$store.state.selectedStrategyData.strategy;\n      }\n    },\n    storedCountries: {\n      get() {\n        return this.$store.state.storedCountries;\n      }\n    },\n    storedChannels: {\n      get() {\n        return this.$store.state.storedChannels;\n      }\n    }\n  },\n  mounted() {\n    this.createDataItems();\n  },\n  methods: {\n    createDataItems() {\n      // Table Data - check box AND budget field for each country\n      var index = 0;\n      this.storedCountries.forEach(country => {\n        var item = {\n          'id': country.id,\n          'country': country.name,\n          'include': false,\n          'budget': '',\n          'auto_allocate': false\n        };\n        this.tableItems.push(item);\n      });\n    },\n    budgetFieldChange(currentItem, index, event) {\n      let currentCountryCode = this.storedCountries[index].code;\n      let savedObj = this.countryChannelData.find(savedItem => savedItem.channelCode === currentItem.channelCode && savedItem.countryCode === currentCountryCode);\n      savedObj.countryChannelBudget = +event;\n    },\n    save() {\n      // Build array of countries\n      var targetCountries = [];\n      console.log('save: ', this.tableItems);\n      this.tableItems.forEach(item => {\n        if (item.include) {\n          let itemObj = {\n            strategy: this.selectedStrategy.id,\n            country: item.id,\n            budget: item.budget,\n            auto_allocate: item.auto_allocate,\n            budget_allocated: 0\n          };\n          targetCountries.push(itemObj);\n        }\n      });\n      console.log(targetCountries);\n      this.$emit('update-target-countries', targetCountries);\n    }\n  }\n};","map":{"version":3,"mappings":";AAyGA;AAEA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC,eACA;QACAC;QACAC;QACAC;QACAC;QACAC;MACA,GACA;QAAAJ;QAAAC;QAAAE;QAAAC;MAAA,GACA;QAAAJ;QAAAC;QAAAE;QAAAC;MAAA,GACA;QAAAJ;QAAAC;QAAAE;QAAAC;MAAA,EACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;MACA;IACA;EACA;EACAC;IACAC;MACAC;QACA;MACA;IACA;IACAC;MACAD;QACA;MACA;IACA;IACAE;MACAF;QACA;MACA;IACA;IACAG;MACAH;QACA;MACA;IACA;EACA;EACAI;IACA;EACA;EACAC;IACAC;MACA;MACA;MACA;QACA;UACA;UACA;UACA;UACA;UACA;QACA;QACA;MACA;IACA;IACAC;MACA;MACA,yDACAC;MACAC;IACA;IACAC;MACA;MACA;MACAC;MACA;QACA;UACA;YACAC;YACAC;YACAC;YACAC;YACAC;UACA;UACAC;QACA;MACA;MACAN;MACA;IACA;EACA;AACA","names":["name","components","CardIcons","data","tableHeaders","text","align","sortable","value","class","tableItems","tableItemLabels","budgetValues","locked","valid","rules","required","notEmpty","inRange","computed","selectedPhase","get","selectedStrategy","storedCountries","storedChannels","mounted","methods","createDataItems","budgetFieldChange","savedItem","savedObj","save","console","strategy","country","budget","auto_allocate","budget_allocated","targetCountries"],"sourceRoot":"src/components/country","sources":["CountryCard.vue"],"sourcesContent":["<template>\r\n    <v-form\r\n        ref=\"form\"\r\n        v-model=\"valid\">\r\n        <v-card class=\"mt-3 mx-3\">\r\n            <v-card-title>\r\n                Select Countries and Set Budgets\r\n                <v-spacer></v-spacer>\r\n                <CardIcons\r\n                    :showEditIcon=\"true\"\r\n                    :showHideIcon=\"true\"\r\n                    @edit=\"(locked = !locked)\"\r\n                    @hide=\"$emit('hide')\"\r\n                    cardType=\"Linked Countries\" />\r\n            </v-card-title>\r\n            <v-card-text class=\"py-0\">\r\n                Phase: {{ this.selectedPhase.name }}&emsp; &emsp;&emsp;Budget: {{ this.selectedPhase.budget }}\r\n            </v-card-text>\r\n            <v-card-text class=\"py-0\">\r\n                Strategy: {{ this.selectedStrategy.name }}&emsp; &emsp;&emsp;Budget: {{ this.selectedStrategy.budget }}\r\n            </v-card-text>\r\n\r\n            <v-card-text>\r\n                <v-sheet\r\n                    class=\"mb-3 grey\"\r\n                    tile\r\n                    outlined>\r\n\r\n                    <v-data-table\r\n                        :headers=\"tableHeaders\"\r\n                        :items=\"tableItems\"\r\n                        hide-default-footer\r\n                        class=\"pb-3\">\r\n\r\n                        <template v-slot:body=\"{ items }\">\r\n                            <tbody>\r\n\r\n                                <tr v-for=\"(item, itemIndex) in items\" :key=\"itemIndex\">\r\n                                    <!-- <td>{{ item.country }}</td> -->\r\n                                    <td>\r\n                                        <v-text-field\r\n                                            :value=\"item.country\"\r\n                                            dense\r\n                                            disabled\r\n                                            solo\r\n                                            flat>\r\n                                        </v-text-field>\r\n\r\n                                    </td>\r\n                                    <td class=\"\">\r\n                                        <v-checkbox\r\n                                            v-model=\"item.include\"\r\n                                            color=\"secondary\"\r\n                                            dense\r\n                                            :readonly=\"locked\"></v-checkbox>\r\n                                    </td>\r\n                                    <td>\r\n                                        <v-text-field\r\n                                            :disabled=\"!item.include\"\r\n                                            :readonly=\"locked\"\r\n                                            v-model.number=\"item.budget\"\r\n                                            label=\"Strategy / Country Budget\"\r\n                                            :rules=\"[rules.notEmpty, rules.inRange]\"\r\n                                            type=\"number\"\r\n                                            dense\r\n\r\n                                            class=\"shrink rounded-0\"\r\n                                            outlined>\r\n                                        </v-text-field>\r\n                                    </td>\r\n                                    <td class=\"pl-7\">\r\n                                        <v-checkbox\r\n                                            v-model=\"item.auto_allocate\"\r\n                                            :disabled=\"item.budget <= 0\"\r\n                                            :readonly=\"locked\"\r\n                                            dense\r\n                                            color=\"secondary\"></v-checkbox>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </template>\r\n                    </v-data-table>\r\n                </v-sheet>\r\n            </v-card-text>\r\n            <v-card-actions v-if=\"!locked\">\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                    class=\"grey lighten-1 mr-3 rounded-0 text-capitalize\"\r\n                    light\r\n                    depressed\r\n                    @click=\"locked = true\">\r\n                    Cancel\r\n                </v-btn>\r\n                <v-btn\r\n                    class=\"mr-3 success rounded-0 text-capitalize\"\r\n                    depressed\r\n                    :disabled=\"!valid\"\r\n                    @click=\"save\">\r\n                    Save\r\n                </v-btn>\r\n            </v-card-actions>\r\n        </v-card>\r\n    </v-form>\r\n</template>\r\n<script>\r\nimport CardIcons from '@/components/shared/CardIcons'\r\n\r\nexport default {\r\n    name: 'CountryCard',\r\n    components: {\r\n        CardIcons,\r\n    },\r\n    data() {\r\n        return {\r\n            tableHeaders: [\r\n                {\r\n                    text: 'Country',\r\n                    align: 'center',\r\n                    sortable: false,\r\n                    value: 'country',\r\n                    class: \"text-subtitle-1 black--text\",\r\n                },\r\n                { text: 'Include?', align: 'start', value: 'include', class: \"text-subtitle-1 black--text\" },\r\n                { text: 'Budget', align: 'center', value: 'budget', class: \"text-subtitle-1 black--text\", },\r\n                { text: 'Auto-allocate?', align: 'start', value: 'autoallocate', class: \"text-subtitle-1 black--text\", },\r\n            ],\r\n            tableItems: [],\r\n            tableItemLabels: [],\r\n            budgetValues: [],\r\n            locked: true,\r\n            valid: false,\r\n            rules: {\r\n                required: (v => !!v || \"Required\"),\r\n                notEmpty: (v => (v.toString().length > 0) || \"Budget should be between 0 and 10,000,000\"),\r\n                inRange: (v => (Number.isInteger(Number(v)) && v >= 0 && v <= 10000000) || \"Budget should be between 0 and 10,000,000\"),\r\n            },\r\n        }\r\n    },\r\n    computed: {\r\n        selectedPhase: {\r\n            get() {\r\n                return this.$store.state.selectedPhaseData.phase\r\n            }\r\n        },\r\n        selectedStrategy: {\r\n            get() {\r\n                return this.$store.state.selectedStrategyData.strategy\r\n            }\r\n        },\r\n        storedCountries: {\r\n            get() {\r\n                return this.$store.state.storedCountries\r\n            }\r\n        },\r\n        storedChannels: {\r\n            get() {\r\n                return this.$store.state.storedChannels\r\n            }\r\n        },\r\n    },\r\n    mounted() {\r\n        this.createDataItems()\r\n    },\r\n    methods: {\r\n        createDataItems() {\r\n            // Table Data - check box AND budget field for each country\r\n            var index = 0\r\n            this.storedCountries.forEach((country) => {\r\n                var item = {\r\n                    'id': country.id,\r\n                    'country': country.name,\r\n                    'include': false,\r\n                    'budget': '',\r\n                    'auto_allocate': false,\r\n                }\r\n                this.tableItems.push(item)\r\n            })\r\n        },\r\n        budgetFieldChange(currentItem, index, event) {\r\n            let currentCountryCode = this.storedCountries[index].code\r\n            let savedObj = this.countryChannelData.find((savedItem) =>\r\n                savedItem.channelCode === currentItem.channelCode && savedItem.countryCode === currentCountryCode)\r\n            savedObj.countryChannelBudget = +event\r\n        },\r\n        save() {\r\n            // Build array of countries\r\n            var targetCountries = []\r\n            console.log('save: ', this.tableItems)\r\n            this.tableItems.forEach((item) => {\r\n                if (item.include) {\r\n                    let itemObj = {\r\n                        strategy: this.selectedStrategy.id,\r\n                        country: item.id,\r\n                        budget: item.budget,\r\n                        auto_allocate: item.auto_allocate,\r\n                        budget_allocated: 0,\r\n                    }\r\n                    targetCountries.push(itemObj)\r\n                }\r\n            })\r\n            console.log(targetCountries)\r\n            this.$emit('update-target-countries', targetCountries)\r\n        },\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}